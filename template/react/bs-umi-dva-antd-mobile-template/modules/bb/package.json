        "prop-types": "^15.5.10",
        "qs": "^6.5.0",
        "react-document-title": "^2.0.3",
        "react-intl": "^2.4.0"
    },
    "devDependencies": {
        "babel-plugin-dev-expression": "^0.2.1",
