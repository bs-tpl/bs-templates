
{
  "presets": [
    "umi/babel"
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "utils": "./src/utils",
          "enums": "./src/utils/enums",
          "services": "./src/services",
          "models": "./src/models",
          "routes": "./src/routes"
        }
      }
    ]
  ]
}
